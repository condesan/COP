  ";s:7:"attribs";a:0:{}s:8:"xml_base";s:30:"http://infoandina.org/?keys%3D";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:0:"";a:6:{s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:210:"Vulnerabilidad y Adaptación al Cambio y Variabilidad Climática de los Sistemas Alimentarios en Zonas Semiáridas de Montaña. Sistematización de una experiencia de consultas a diferentes niveles de decisión";s:7:"attribs";a:0:{}s:8:"xml_base";s:30:"http://infoandina.org/?keys%3D";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:32:"http://infoandina.org/node/28340";s:7:"attribs";a:0:{}s:8:"xml_base";s:30:"http://infoandina.org/?keys%3D";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:3443:"<div class="field field-type-text field-field-autores">
